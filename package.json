{
  "name": "linkedin-api-backend",
  "version": "1.0.0",
  "description": "LinkedIn-like API backend with Sequelize ORM, JWT authentication, and TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts --exec ts-node",
    "dev:watch": "nodemon --watch src --ext ts --exec yarn dev:ts",
    "dev:ts": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "rebuild": "yarn clean && yarn build",
    "type-check": "tsc --noEmit",
    "lint": "echo 'No linter configured yet'",
    "test": "echo 'No tests configured yet'"
  },
  "keywords": [
    "linkedin",
    "api",
    "backend", 
    "sequelize",
    "jwt",
    "typescript",
    "express",
    "mysql"
  ],
  "author": "Ameer Uqba Goraya <ameer.uqba@dubizzlelabs.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=1.22.0"
  },
  "packageManager": "yarn@1.22.22",
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/sequelize": "^4.28.20",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
